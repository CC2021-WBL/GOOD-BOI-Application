{
    "extends" : [
        "eslint : recommended",
        "plugin: import/errors",
        "plugin: react/recommended",
        "plugin: jsx-a11y/recommended",
        "plugin: react-hoks/recommended",
        "prettier"
    ],
    "plugins" :[
        "react",
        "import",
        "jsx-a11y"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures" :{
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
"react/prop-types": 2,
"react/react-in-jsx-scope": 1
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}