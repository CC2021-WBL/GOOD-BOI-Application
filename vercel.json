{
  "version": 2,
  "builds": [
    { "src": "client/package.json", "use": "@vercel/static-build" },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    { "src": "/api/.*", "dest": "/api/server.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
